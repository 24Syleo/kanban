{% extends 'base.html.twig' %}

{% block title %}TaskFiles index{% endblock %}

{% block body %}
    <h1>TaskFiles index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>FilesName</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for task_file in task_files %}
            <tr>
                <td>{{ task_file.id }}</td>
                <td>{{ task_file.filesName }}</td>
                <td>
                    <a href="{{ path('app_task_files_show', {'id': task_file.id}) }}">show</a>
                    <a href="{{ path('app_task_files_edit', {'id': task_file.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_task_files_new') }}">Create new</a>
{% endblock %}
