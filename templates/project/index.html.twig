<h1>Project index</h1>

<table class="table">
	<thead>
		<tr>
			<th>Id</th>
			<th>Title</th>
			<th>CreatedAt</th>
			<th>actions</th>
		</tr>
	</thead>
	<tbody>
		{% for project in projects %}
			<tr>
				<td>{{ project.id }}</td>
				<td>{{ project.title }}</td>
				<td>{{ project.createdAt ? project.createdAt|date('Y-m-d H:i:s') : '' }}</td>
				<td>
					<a class="btn btn-primary" href="{{ path('project.show', {'id': project.id}) }}">show</a>
					<a class="btn btn-info" href="{{ path('project.edit', {'id': project.id}) }}">edit</a>
				</td>
			</tr>
		{% else %}
			<tr>
				<td colspan="4">no records found</td>
			</tr>
		{% endfor %}
	</tbody>
</table>

<a class="btn btn-success" href="{{ path('project.new') }}">Create new</a>
